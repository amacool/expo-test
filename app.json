{
  "expo": {
    "name": "PetsIdApp",
    "description": "A very interesting project.",
    "slug": "petsidapp",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#F75356"
    },
    "assetBundlePatterns": [
      "./src/assets/*",
      "./node_modules/react-navigation/src/**/*.png"
    ],
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.petsid.app"
    },
    "android": {
      "package": "com.petsid.app"
    }
  }
}
